name = ["Ashik","Rofiq","shofiq","Ratul"]
s = "boltu" 
i = 0  
while i < len(name) : 
    if name[i] == s :
     break
    i = i - 1
else : 
    print(s," is not available")

name = ["Ashik","Rofiq","shofiq","Ratul"]
while len(name) : 
    print(name.pop())
 
